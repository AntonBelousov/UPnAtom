{
  "name": "UPnAtom",
  "version": "0.0.1.beta.5",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "An open source Universal Plug and Play library with a focus on media streaming coordination using the UPnP A/V profile.",
  "homepage": "https://github.com/master-nevi/UPnAtom",
  "authors": {
    "David Robles": "master-nevi@users.noreply.github.com"
  },
  "source": {
    "git": "https://github.com/master-nevi/UPnAtom.git",
    "tag": "0.0.1.beta.5"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  },
  "default_subspecs": "Default",
  "subspecs": [
    {
      "name": "Default",
      "source_files": "Source/UPnAtom.swift",
      "dependencies": {
        "UPnAtom/Core": [

        ],
        "UPnAtom/AV Profile": [

        ]
      }
    },
    {
      "name": "UPnP Foundation",
      "source_files": "Source/UPnP Foundation/*.swift"
    },
    {
      "name": "SSDP Explorer",
      "source_files": "Source/SSDP/Explorer/*.swift",
      "dependencies": {
        "UPnAtom/UPnP Foundation": [

        ],
        "CocoaAsyncSocket": [
          "~> 7.4.1"
        ],
        "AFNetworking/Serialization": [
          "~> 2.5.0"
        ]
      }
    },
    {
      "name": "Core",
      "source_files": [
        "Source/{HTTP Client Session Managers and Serializers,Logging,Management,Parsers,UPnP Objects}/*.swift",
        "Source/SSDP/Discovery Adapter/*.swift"
      ],
      "dependencies": {
        "UPnAtom/SSDP Explorer": [

        ],
        "AFNetworking": [
          "~> 2.5.0"
        ],
        "Ono": [
          "~> 1.2.0"
        ],
        "GCDWebServer": [
          "~> 3.2.2"
        ]
      }
    },
    {
      "name": "AV Profile",
      "source_files": "Source/AV Profile/**/*.swift",
      "dependencies": {
        "UPnAtom/Core": [

        ]
      }
    },
    {
      "name": "CocoaSSDP Support",
      "source_files": "Source/CocoaSSDP Support/*.swift",
      "dependencies": {
        "UPnAtom/Core": [

        ],
        "CocoaSSDP": [
          "~> 0.1.0"
        ]
      }
    }
  ]
}
